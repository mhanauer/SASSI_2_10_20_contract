#describe(clinical_sample)

#client setting; 1=criminal setting, 2=social services program, 3=medical pain clinic, 4=medical facility (not pain), 6=substance use treatment, 7=Other
describe.factor(clincal_sample$CLIENTSETTING)

#clinical diagnosis
describe.factor(clincal_sample$NODIAG)
clincal_sample$diag_totals = clincal_sample$ALCTO + clincal_sample$POTTOT + clincal_sample$HALLUCTOT + clincal_sample$INHALTOT + clincal_sample$OPIOIDTOT + clincal_sample$SEDTOT + clincal_sample$SEDTOT + clincal_sample$STIMTOT + clincal_sample$OTHRDRUGTOT

describe.factor(clincal_sample$NODIAG)
#257 total in development sample

diagnoses=subset(clincal_sample$diag_totals,clincal_sample$diag_totals>=2)
