describe.factor(stability_sample$TN_WORK_R)

#Race/ethnicity; 1=American Indian/Alaskan Native, 2=Asian, 3=Black/African american, 4=White, 5=Hispanic (per email), 6=Native Hawaiian/Pacific Islander, 7=Multiracial, 8=Other
describe.factor(stability_sample$TN_RACEETH_R)

#Living situation; 9=parents, 10=Other relatives, 11=friends, 12=group home, 13=residential, 14=other
#no data

#B5 is trouble with the law
#no B5/no data

#education
describe.factor(clincal_sample$YEARS_ED)
clincal_sample$education = clincal_sample$YEARS_ED
clincal_sample$education <- (ifelse(clincal_sample$education == 28, 5, ifelse(clincal_sample$education == 29, 6, ifelse(clincal_sample$education == 30,7, ifelse(clincal_sample$education==31, 8, ifelse(clincal_sample$education == 32, 9, ifelse(clincal_sample$education ==33, 10, ifelse(clincal_sample$education == 34, 11, ifelse(clincal_sample$education==35,12,clincal_sample$education)))))))))
