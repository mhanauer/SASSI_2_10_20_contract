stability_sample$education = stability_sample$TN_EDUC_R
stability_sample$education <- (ifelse(stability_sample$education == 1, 5, ifelse(stability_sample$education == 2, 6, ifelse(stability_sample$education == 3,7, ifelse(stability_sample$education==4, 8, ifelse(stability_sample$education == 5, 9, ifelse(stability_sample$education ==6, 10, ifelse(stability_sample$education == 7, 11, ifelse(stability_sample$education==8,12,stability_sample$education)))))))))
stability_sample$education[stability_sample$education==36] <- NA

